    "url": "https://github.com/rifat-H/Bootstrap-template/issues"
  },
  "homepage": "https://github.com/rifat-H/Bootstrap-template#readme",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.0",
    "popper.js": "^1.15.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "autoprefixer": "^9.5.1",
    "cssnano": "*",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-purgecss": "^2.1.0",
    "rimraf": "*",
